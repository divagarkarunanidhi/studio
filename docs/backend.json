{
  "entities": {
    "defectData": {
      "title": "Defect Data",
      "description": "A collection of defects for a user.",
      "type": "object",
      "properties": {
        "defects": {
          "type": "array",
          "description": "A list of defects.",
          "items": {
            "type": "object"
          }
        },
        "uploadedAt": {
          "type": "string",
          "format": "date-time",
          "description": "The timestamp when the data was uploaded."
        }
      },
      "required": ["defects", "uploadedAt"]
    }
  },
  "auth": {
    "providers": ["google.com"]
  },
  "firestore": {
    "/defects/{userId}": {
      "schema": {
        "$ref": "#/backend/entities/defectData"
      },
      "description": "Stores the defect data for a specific user."
    }
  }
}
